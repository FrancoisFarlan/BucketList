{% extends 'base.html.twig' %}

{% block body %}

    <section class="team-members">
        {% for member in teamMembers %}
            <article>
                <div>
                    <h3>{{ member.firstname }} {{ member.lastname }}</h3>
                    <p>Date of birth: {{ member.dateOfBirth|date("d-m-Y") }}</p>
                    <p>{{ member.description }}</p>
                </div>
            </article>
        {% endfor %}
    </section>
{% endblock %}

{% block title %}

    {{ parent() }} | A propos
{% endblock %}

